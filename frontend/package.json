{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "cross-env VITE_VISUALIZER=true vite build",
    "preview": "vite preview",
    "preview:host": "vite preview --host --port 5173",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.ts,.js --max-warnings=0",
    "lint:fix": "eslint . --ext .vue,.ts,.js --fix",
    "format": "prettier . --write",
    "postbuild": "node scripts/after-build.ts"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "@unhead/vue": "^2.0.19",
    "cross-env": "^7.0.3",
    "electron-devtools-installer": "^4.0.0",
    "element-plus": "2.11.3",
    "unplugin-vue-markdown": "^29.2.0",
    "vite-plugin-pages": "^0.33.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/node": "^24.7.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^9.28.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^29.1.0",
    "vite": "7.1.11",
    "vite-plugin-vue-layouts-next": "^1.0.0",
    "vite-svg-loader": "^5.1.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.1"
  }
